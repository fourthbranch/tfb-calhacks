{
  "version": 2,
  "builds": [
    {
      "src": "backend/vercel_handler.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11",
        "requirementsPath": "requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/vercel_handler.py"
    }
  ],
  "functions": {
    "backend/vercel_handler.py": {
      "runtime": "python3.11"
    }
  }
}
